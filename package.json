{
  "name": "celebrityskateboards-spa-angular",
  "version": "0.0.0",
  "homepage": ".",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disable-host-check --proxy-config ./proxy.config.json",
    "build": "rm -rf dist && ng build --prod --build-optimizer --aot",
    "test": "ng test --watch=false --browsers ChromeHeadless --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docker:build": "docker build ./ -t celebrityskateboards-spa-angular --no-cache",
    "docker:tag": "docker tag celebrityskateboards-spa-angular us.gcr.io/${GCLOUD_PROJECT_ID}/celebrityskateboards-spa-angular:$(git rev-parse HEAD)",
    "docker:publish": "gcloud docker -- push us.gcr.io/${GCLOUD_PROJECT_ID}/celebrityskateboards-spa-angular:$(git rev-parse HEAD)",
    "set-version": "sed -i -e 's/^version:.*$/version: '$(date '+%Y.%m%d.%H%M')'/' -e 's/^appVersion:.*$/appVersion: '$(git rev-parse HEAD)'/' .helm/Chart.yaml",
    "deploy": "helm upgrade celebrityskateboards .helm"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^0.803.29",
    "@angular/animations": "^8.2.6",
    "@angular/cdk": "^8.2.0",
    "@angular/common": "^8.2.6",
    "@angular/compiler": "^8.2.6",
    "@angular/core": "^8.2.6",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "^8.2.6",
    "@angular/http": "^7.2.15",
    "@angular/material": "^8.2.0",
    "@angular/platform-browser": "^8.2.6",
    "@angular/platform-browser-dynamic": "^8.2.6",
    "@angular/router": "^8.2.6",
    "core-js": "^3.21.1",
    "node-sass": "^4.13.1",
    "rxjs": "^6.6.7",
    "rxjs-compat": "^6.5.3",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular/cli": "^8.3.4",
    "@angular/compiler-cli": "^8.2.6",
    "@angular/language-service": "^13.3.6",
    "@types/jasmine": "^3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.7.5",
    "codelyzer": "~5.1.0",
    "install": "^0.13.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.3.17",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-coveralls": "^2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "npm": "^8.10.0",
    "protractor": "^5.4.2",
    "ts-node": "~8.3.0",
    "tslint": "~5.20.0",
    "typescript": "3.5.3",
    "webpack-dev-server": "^3.8.0"
  }
}
